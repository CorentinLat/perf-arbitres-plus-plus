  },
  "engines": {
    "node": ">=14.0.0"
  },
  "devEngines": {
    "node": ">=14.x",
    "npm": ">=7.x"
  },
  "electronmon": {
    "patterns": [
      "!**/**",
      "src/electron/**"
    ],
    "logLevel": "quiet"
  },
  "browserslist": [
    "chrome 100"
  ]
